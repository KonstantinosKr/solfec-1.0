\end_layout

\begin_layout LyX-Code
7.1   SWEEP 
\begin_inset Formula $\left(Middle_{i}\right)$
\end_inset


\end_layout

\begin_layout LyX-Code
7.2   send 
\begin_inset Formula $\left(Middle_{i}\right)$
\end_inset


\end_layout

\begin_layout LyX-Code
